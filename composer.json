{
  "name": "ssitdikov/atol",
  "description": "ATOL PHP API Client. Данная библиотека предназначена для работы с сервисом АТОЛ Онлайн",
  "keywords": ["atol", "api", "online", "php"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Salavat Sitdikov",
      "email": "sitsalavat@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "SSitdikov\\ATOL\\": "src/",
      "SSitdikov\\ATOL\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpcov": "^5.0",
    "codacy/coverage": "^1.4",
    "phpmd/phpmd": "^2.6"
  }
}
